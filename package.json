{
  "name": "components-kit",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages --out=.out"
  },
  "dependencies": {
    "dayjs": "^1.10.6",
    "vue": "^3.0.5",
    "vue3-click-away": "^1.2.1"
  },
  "main": "./dist/components-kit.umd.js",
  "module": "./dist/components-kit.es.js",
  "exports": {
    ".": {
      "import": "./dist/components-kit.es.js",
      "require": "./dist/components-kit.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@storybook/addon-a11y": "^6.3.4",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/storybook-deployer": "^2.8.10",
    "@storybook/vue3": "^6.3.4",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-standard": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "eslint": "^7.28.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.11.1",
    "postcss": "^8.3.5",
    "tailwindcss": "^2.2.2",
    "typescript": "^4.3.2",
    "vite": "^2.3.7",
    "vue-loader": "^16.2.0",
    "vue-tsc": "^0.0.24"
  }
